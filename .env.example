OPENAI_API_KEY=sk-
OPENROUTER_API_KEY=sk-or-
GEMINI_API_KEY=AIza
XAI_API_KEY=
GROQ_API_KEY=gsk_
CEREBRAS_API_KEY=
MISTRAL_API_KEY=
KIMI_API_KEY=
MOONSHOT_API_KEY=
DEEPSEEK_API_KEY=sk-
HUGGINGFACE_HUB_TOKEN=hf_

# === CLAUDE OAUTH (use Pro/Max subscription) ===
CLAUDE_CODE_OAUTH_TOKEN=sk-ant-oat01-

# === OLLAMA (local models) ===
